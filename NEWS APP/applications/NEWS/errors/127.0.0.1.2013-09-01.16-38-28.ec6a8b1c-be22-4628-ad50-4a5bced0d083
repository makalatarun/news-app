rud, service, plugins = Crud(db), Service(), PluginManager()\n\n## create all tables needed by auth if not custom tables\n\n\n\nauth.define_tables(username=False, signature=False)\n\n\n## configure email\nmail = auth.settings.mailer\nmail.settings.server = 'logging' or 'smtp.gmail.com:587'\nmail.settings.sender = 'you@gmail.com'\nmail.settings.login = 'username:password'\n\n## configure auth policy\nauth.settings.registration_requires_verification = False\nauth.settings.registration_requires_approval = False\nauth.settings.reset_password_requires_verification = True\n\n## if you need to use OpenID, Facebook, MySpace, Twitter, Linkedin, etc.\n## register with janrain.com, write your domain:api_key in private/janrain.key\nfrom gluon.contrib.login_methods.rpx_account import use_janrain\nuse_janrain(auth, filename='private/janrain.key')\n\n#########################################################################\n## Define your tables below (or better in another model file) for example\n##\n## >>> db.define_table('mytable',Field('myfield','string'))\n##\n## Fields can be 'string','text','password','integer','double','boolean'\n##       'date','time','datetime','blob','upload', 'reference TABLENAME'\n## There is an implicit 'id integer autoincrement' field\n## Consult manual for more options, validators, etc.\n##\n## More API examples for controllers:\n##\n## >>> db.mytable.insert(myfield='value')\n## >>> rows=db(db.mytable.myfield=='value').select(db.mytable.ALL)\n## >>> for row in rows: print row.id, row.myfield\n#########################################################################\n\n## after defining tables, uncomment below to enable auditing\n# auth.enable_record_versioning(db)\nimport datetime\ndb.define_table('category',Field('name','string',requires=IS_NOT_IN_DB(db,'category.name')))\ndb.define_table('news',Field('Category','string','reference category',requires=IS_IN_DB(db,'category.id','category.name')),Field('Heading','string',requires=IS_NOT_EMPTY()),Field('Url','string',requires=IS_URL()),Field('Description','text',default=None),Field('posted_by','string','reference auth_user'),Field('posted_on','datetime',default=datetime.datetime.now()),Field('likes','integer',default=0),Field('dislikes','integer',default=0),Field('ranks','integer',default=0),Field('want to embed the video'))\ndb.define_table('comments',Field('new_id','reference news'),Field('body','text'),Field('commented_by','reference auth_user' ),Field('commented_on','datetime',default=datetime.datetime.now()))\ndb.define_table('LD',Field('usr','reference auth_user'),Field('new_id','reference news'),Field('l','boolean',default=False),Field('d','boolean',default=False))\n#db.news.posted_by.default = auth.user.id if auth.user else 0\n#db.news.posted_by.writable=db.news.posted_by.readable=False\n#db.comments.posted_by.default = auth.user.id if auth.user else 0\n#db.comments.commented_by.writable=db.comments.commented_by.readable=False\nauth.settings.logout_next = URL('home')\nauth.settings.login_next = URL('home')\nauth.settings.register_next = URL('home')\n"
